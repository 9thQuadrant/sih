<?xml version='1.0' encoding='utf-8'?>
<widget id="cec.tel.med" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="MediaStreams">
        <param name="android-package" value="com.adobe.phonegap.media.MediaStreamsPlugin" />
    </feature>
    <name>Tele Medicine</name>
    <description>
        Tele Medicine App that uses less medicine.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        9thQuadrant Inc.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="icons/icon96.png" />
    <icon density="mdpi" src="icons/icon96.png" />
    <icon density="hdpi" src="icons/icon96.png" />
    <icon density="xhdpi" src="icons/icon96.png" />
    <icon density="xxhdpi" src="icons/icon96.png" />
    <icon density="xxxhdpi" src="icons/icon96.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
